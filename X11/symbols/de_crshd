    include "de(nodeadkeys)"

    name[Group1]="German";

    key <AC10>	{ [bracketleft, braceleft,           odiaeresis,    Odiaeresis ] };
    key <AC11>	{ [bracketright, braceright,         adiaeresis, Adiaeresis ] };
};

