;;; Installation:
;;
;; Drop the theme in a folder that is on 'custom-theme-load-path'
;; and enjoy!
;;
;; Don't forget that the theme requires Emacs 24.
;;

;;; Code

(deftheme solarized-grey
  "erosion-theme, generated by colorbuild")

(let ((class '((class color) (min-colors 89)))

      ;; (base00    "#262626")
      ;; (base01    "#303030")
      ;; (base02    "#6a6a6a")
      ;; (base03    "#777777")
      ;; (base04    "#909090")
      ;; (base05    "#9e9e9e")
      ;; (base06    "#e2e2e2")
      ;; (base07    "#f6f6f6")

      (ml-inactive "#1b1b1b")
      (ml-active   "#222222")

      (base07    "#262626")
      (base06    "#303030")
      (base05    "#6a6a6a")
      (base04    "#777777")
      (base03    "#909090")
      (base02    "#9e9e9e")
      (base01    "#e2e2e2")
      (base00    "#f6f6f6")

      (ml-inactive base01)
      (ml-active   base02)

      (accent00  "#e36f3e")
      (accent01  "#d53f58")
      (accent02  "#d33b7c")
      (accent03  "#a258bf")
      (accent04  "#4372de")
      (accent05  "#009da5")
      (accent06  "#00a978")
      (accent07  "#be8612")
			
      (accent-red    "#e36f3e")
      (accent-green  "#00a978")
      (accent-yellow "#be8612")
      (accent-blue   "#a258bf"))

  (defface mode-line-buffer-name
    '((t :foreground "green")) "Custom Face")
  (defface mode-line-mode
    '((t :foreground "yellow")) "Custom Face")
  (defface mode-line-minor-mode
    '((t :foreground "gray20")) "Custom Face")
  (defface mode-line-line
    '((t :foreground "magenta")) "Custom Face")
  (defface mode-line-func
    '((t :foreground "blue")) "Custom Face")
  (defface mode-line-col
    '((t :foreground "green")) "Custom Face")
  (defface mode-line-80col
    '((t :foreground "red")) "Custom Face")

  (custom-theme-set-faces
   'solarized-grey

   ;;; basic coloring
   `(default ((,class (:foreground ,base05 :background ,base00))))
